{
  "name": "skeleton",
  "description": "The CLI for Skeleton related tooling.",
  "version": "4.4.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skeletonlabs/skeleton.git",
    "directory": "packages/cli"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "bin": "./dist/index.mjs",
  "files": [
    "dist",
    "!dist/**/*.map"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@types/estree": "1.0.8",
    "@types/node": "24.10.1",
    "@types/semver": "7.7.1",
    "tsdown": "0.16.4",
    "type-fest": "5.2.0",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "vitest": "4.0.9"
  },
  "dependencies": {
    "@clack/prompts": "0.11.0",
    "colorette": "2.0.20",
    "commander": "14.0.2",
    "detect-indent": "7.0.2",
    "latest-version": "9.0.0",
    "magic-string": "0.30.21",
    "nanoid": "5.1.6",
    "node-html-parser": "7.0.1",
    "package-manager-detector": "1.5.0",
    "postcss": "8.5.6",
    "semver": "7.7.3",
    "svelte": "5.43.7",
    "tinyglobby": "0.2.15",
    "ts-morph": "27.0.2",
    "zimmerframe": "1.1.4"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "test": "vitest run",
    "check": "tsc",
    "sync": "pnpm build"
  }
}